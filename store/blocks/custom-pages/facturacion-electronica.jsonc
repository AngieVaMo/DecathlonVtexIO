    "props": {
      "blockClass": "image__fe--one",
      "src": "assets/img/facturacion-electronica-one.png"
    }
  },
  "rich-text#text__fe--five": {
    "props": {
      "blockClass": "text__fe--five",
      "text": "Este es el paso a paso si tu compra fue en nuestras tiendas físicas:"
    }
  },
  "image#image__fe--two": {
