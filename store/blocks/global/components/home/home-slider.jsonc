{
    "slider-layout#home__slider--productos-temporada": {
        "title": "Slider con hijos declarados",
        "props": {
          "itemsPerPage": {
            "desktop": 5,
            "tablet": 3,
            "mobile": 3
          },
          "infinite": true,
          "blockClass": "home__slider",
          "autoplay":{
            "timeout": 3000,
            "stopOnHover": true
          }
        },
        "children": [
          //"product-summary.shelf"
          "image#home__slider--one",
          "image#home__slider--two",
          "image#home__slider--three",
          "image#home__slider--four",
          "image#home__slider--five",
          "image#home__slider--six",
          "image#home__slider--seven"
        ]
      },
      
        "image#home__slider--one":{
          "title": "Imagen bandas elásticas",
          "props":{
            "src":"assets/img/main-slide-1.jpg"
          }
        },
