{
  "flex-layout.row#product__tabs": {
    "title": "Información adicional",
    "children": [
      "tab-layout#product__tabs"
    ]
  },
  "tab-layout#product__tabs": {
    "children": [
      "tab-list#product__tabs--list",
      "tab-content#product__tabs--content"
    ]
  },


  "tab-list#product__tabs--list": {
    "children": [
      "tab-list.item#tabs__list--beneficios",
      "tab-list.item#tabs__list--info-tecnica",
      "tab-list.item#tabs__list--reseñas"
      //"tab-list.item#tabs__list--questions"
    ]
  },
  "tab-list.item#tabs__list--beneficios": {
    "props": {
      "tabId": "beneficios",
      "label": "Beneficios",
      "defaultActiveTab": false
    }
  },
  "tab-list.item#tabs__list--info-tecnica": {
    "props": {
      "tabId": "product-specification",
      "label": "Información técnica",
      "defaultActiveTab": false
    }
  },
  "tab-list.item#tabs__list--reseñas": {
    "props": {
      "tabId": "reseñas",
      "label": "Reseñas",
      "defaultActiveTab": false
    }
  },
  "tab-list.item#tabs__list--questions": {
    "props": {
      "tabId": "questions",
      "label": "Preguntas y respuestas",
      "defaultActiveTab": false
    }

  },


  "tab-content#product__tabs--content": {
    "children": [
      //"tab-content.item#tabs__content--info-tecnica",
      "tab-content.item#tabs__content--reseñas"
      //"tab-content.item#tabs__content--questions"
    ]
  },
  "tab-content.item#tabs__content--reseñas": {
    "props": {
      "tabId": "reseñas"
    },
    "children": ["product-reviews"]
  }
 /*"tab-content.item#tabs__content--info-tecnica": {
    "props": {
      "tabId": "info-tecnica"
    },
    "children": ["product-specification"]
  }
  "tab-content.item#tabs__content--questions": {
    "props": {
      "tabId": "questions"
    },
    "children": ["questions-and-answers"]
  }*/
}